beginchar(c_nnu,50uw#+rm#,20uh#,0); "nnu";
    pickup frame_pen;
    % from "malayalam letter nga"
    % first bow
    front_eye(1,2,3,4,5);
    z5=(0, 1/2h);
    z6=(9uw, h);
    z7=(17uw, 1/2h);
    z8=(x7, 0);
    % second bow
    z9=(25uw, h);
    z10=(33uw, 1/2h);
    z11=(x10, 0);
    % third bow
    z12=(41uw, h);
    z13=(50uw, 1/2h);
    end_loop(13,14,15,16);
    
    draw_eye(1,2,3,4);
    draw z4{left}..z5{up}..z6{right}..z7{down}---z8;
