if unknown subfont:
beginchar(c_ku,30uw#+rm#,20uh#,0); "ku";
    pickup frame_pen;
    x6 = 0;
    letter_ka(1,2,3,4,4',5,6,7,8',9',10',11');
    % three-like shape for "u"
    z8=(x10, y1);
    z9=(30uw, y10+2.75uw);
    z10=(x9 - 3.5uw, y6);
    z11=(x10 - .5uw, y6);
    z12=(x9, y10-2.75uw);
